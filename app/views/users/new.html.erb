<%= @user.errors.full_messages %>
<%= form_with model: @user, local: true do |f| %>
  <div class="field-row">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field-row">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <%= f.submit "Signup" %>
<% end %>
